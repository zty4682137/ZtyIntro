<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="Generator" content="EditPlus®">
  <meta name="Author" content="">
  <meta name="Keywords" content="">
  <meta name="Description" content="">
  <title>我是京东的登录页</title>
 
	<style>
		#header
		{
			display:block;
			background:#eee;
			width:100%;
			height:80px;
			position:relative;
		}
		#header>#img1
		{
			width:170px;
			height:60px;
			background:url("jd_logo.png");
			position:absolute;
			left:50px;
			top:10px;
		}
		#header>#img2
		{
			width:118px;
			height:47px;
			background:url("welcome.png");
			position:absolute;
			left:240px;
			top:16px;
			border-left:2px #888 solid;
		}
		#d1
		{
			height:423px;
			width:900px;
			background:#888;
			border-radius:20px;
			margin:100px auto;
			overflow:hidden;
		}
		#cont
		{
			height:403px;
			width:880px;
			background:#fff;
			margin:10px auto;
			position:relative;
		}
		#d1 #cont #img
		{
			width:511px;
			height:403px;
			position:absolute;
			background:url("jd_show.png");
		}
		#input
		{
			position:absolute;
			right:135px;
			top:80px;
			font-family:"microsoft yahei";
			font-size:12pt;
			color:#777;

			/*border:1px red solid;*/
		}
		#input #text,#input #password
		{
			width:250px;
			height:20px;
		}
		#clienticon
		{
			width:18px;
			height:21px;
			background:url("login_all_bg.png");
			background-position:-345px -108px;
			float:right;
			
		}
		#passicon
		{
			width:18px;
			height:21px;
			background:url("login_all_bg.png");
			background-position:-373px -108px;
			float:right;
			
		}
		#button
		{
			width:308px;
			height:34px;
			background:url("login_all_bg.png");
			background-position:0px -61px;
			position:absolute;
			right:100px;
			bottom:60px;
		}
		#button:hover
		{
			background-position:0px -95px;
		}
		#che
		{
			margin-top:0px;
			height:15px;
			width:15px;
		}
		#chediv
		{
			float:left;
			height:22px;
			width:150px;
			font-family:"microsoft yahei";
			color:#777;
		}
		/*header*/
		/*{*/
			/*background:black;*/
			/*height:200px;*/
			/*width:200px;*/
		/*}*/
	</style>
	
 </head>
 <body>
	<div id="header">
		<div id="img1"></div>
		<div id="img2"></div>
	</div>
	<!-- <header></header> -->
	<div id="d1">
		<div id="cont">
			<div id="img"></div>
			<div id="input">
				<p>用户名</p>
				<input id="text" type="text"/><div id="clienticon"></div><br/>
				<p>密码</p>
				<input id="password" type="password"/><div id="passicon"></div><br/><br/>
				<div id="chediv"><input id="che"type="checkbox" value="羊羊羊"></input>记住密码</div>
				
			</div>
			<div id="button">
				
			</div>
		</div>
	</div>
 </body>
</html>
